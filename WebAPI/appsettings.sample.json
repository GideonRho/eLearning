{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ImageRoot": "<ImageRoot>",
  "TemplatesRoot": "<TemplateRoot>",
  "Email": {
    "Enable": "<EmailEnable>",
    "Redirect": "<EmailRedirect>",
    "RedirectAddress": "<EmailRedirectAddress>",
    "Host": "<EmailHost>",
    "User": "<EmailUser>",
    "Password": "<EmailPassword>",
    "FromAddress": "<EmailFromAddress>",
    "Name": "<EmailName>"
  },
  "SwaggerFile": "/swagger/v1/swagger.json",
  "AllowedHosts": "*",
  "Urls": "http://localhost:5000",
  "Origins": ["http://localhost:3000", "http://localhost:3001", "http://localhost:5000", "<ApiOrigin>", "<AdminOrigin>", "<Origin>"],
  "ConnectionStrings": {
    "DefaultConnection": "Server=<DatabaseHost>;Port=<DatabasePort>;Database=<Database>;User Id=<DatabaseUser>;Password=<DatabasePassword>; sslmode=Require; Trust Server Certificate=true"
  }
}
